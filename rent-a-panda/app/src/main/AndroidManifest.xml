<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jdurieu.rentapanda">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.jdurieu.rentapanda.App"
        android:allowBackup="true"
        android:icon="@mipmap/panda"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.jdurieu.rentapanda.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.jdurieu.rentapanda.activity.JobDetailActivity"></activity>
    </application>

</manifest>